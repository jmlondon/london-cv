@article{vanwormer2019,
  title = {Viral Emergence in Marine Mammals in the {{North Pacific}} May Be Linked to {{Arctic}} Sea Ice Reduction},
  author = {VanWormer, E. and Mazet, J. A. K. and Hall, A. and Gill, V. A. and Boveng, P. L. and London, J. M. and Gelatt, T. and Fadely, B. S. and Lander, M. E. and Sterling, J. and Burkanov, V. N. and Ream, R. R. and Brock, P. M. and Rea, L. D. and Smith, B. R. and Jeffers, A. and Henstock, M. and Rehberg, M. J. and {Burek-Huntington}, K. A. and Cosby, S. L. and Hammond, J. A. and Goldstein, T.},
  year = {2019},
  month = nov,
  journal = {Scientific Reports},
  volume = {9},
  number = {1},
  pages = {15569},
  issn = {2045-2322},
  doi = {10.1038/s41598-019-51699-4},
  abstract = {Climate change-driven alterations in Arctic environments can influence habitat availability, species distributions and interactions, and the breeding, foraging, and health of marine mammals. Phocine distemper virus (PDV), which has caused extensive mortality in Atlantic seals, was confirmed in sea otters in the North Pacific Ocean in 2004, raising the question of whether reductions in sea ice could increase contact between Arctic and sub-Arctic marine mammals and lead to viral transmission across the Arctic Ocean. Using data on PDV exposure and infection and animal movement in sympatric seal, sea lion, and sea otter species sampled in the North Pacific Ocean from 2001\textendash 2016, we investigated the timing of PDV introduction, risk factors associated with PDV emergence, and patterns of transmission following introduction. We identified widespread exposure to and infection with PDV across the North Pacific Ocean beginning in 2003 with a second peak of PDV exposure and infection in 2009; viral transmission across sympatric marine mammal species; and association of PDV exposure and infection with reductions in Arctic sea ice extent. Peaks of PDV exposure and infection following 2003 may reflect additional viral introductions among the diverse marine mammals in the North Pacific Ocean linked to change in Arctic sea ice extent.},
  keywords = {PEP},
  file = {/Users/josh.london/Zotero/storage/IKX3AJM9/VanWormer_2019_Viral.pdf}
}

